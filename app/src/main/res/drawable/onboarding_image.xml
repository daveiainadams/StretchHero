<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="200dp"
    android:height="200dp"
    android:viewportWidth="200"
    android:viewportHeight="200">

    <!-- Background circle -->
    <path
        android:fillColor="@color/light_blue_background"
        android:pathData="M100,100m-90,0a90,90 0,1,1 180,0a90,90 0,1,1 -180,0"/>

    <!-- Person stretching icon - head -->
    <path
        android:fillColor="@color/primary_blue"
        android:pathData="M100,60a15,15 0,1,0 0,-30a15,15 0,1,0 0,30z"/>

    <!-- Person stretching icon - body (filled) -->
    <path
        android:fillColor="@color/primary_blue"
        android:pathData="M100,70 L100,120 M80,90 L100,80 L120,90 M100,120 L85,150 M100,120 L115,150"/>

    <!-- Person stretching icon - body outline (stroked) -->
    <path
        android:strokeColor="@color/primary_blue"
        android:strokeWidth="4"
        android:strokeLineCap="round"
        android:fillColor="#00000000"
        android:pathData="M100,70 L100,120 M80,90 L100,80 L120,90 M100,120 L85,150 M100,120 L115,150"/>
</vector>